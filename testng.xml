<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="DSAlgoTestSuite" parallel="classes" thread-count="1">
   <test name="EdgeTests">
        <parameter name="browserType" value="edge"/>
        <classes>
            <class name="testCases.DsAlgoPortalTestCases"/>
            <class name="testCases.HomePageTestCases"/>
            <class name="testCases.RegisterPageTestCases"/>
            <class name="testCases.SignInPageTestCases"/>
            <class name="testCases.TryEditorTestCases"></class>
        </classes>
    </test>
    <test name="ChromeTests">
        <parameter name="browserType" value="chrome"/>
        <classes>
            <class name="testCases.DsAlgoPortalTestCases"/>
            <class name="testCases.HomePageTestCases"/>
            <class name="testCases.RegisterPageTestCases"/>
            <class name="testCases.SignInPageTestCases"/>
             <class name="testCases.TryEditorTestCases"></class>
        </classes>
    </test>
   <test name="FirefoxTests">
       <parameter name="browserType" value="firefox"/>
      <classes>
           <class name="testCases.DsAlgoPortalTestCases"/>
            <class name="testCases.HomePageTestCases"/>
            <class name="testCases.RegisterPageTestCases"/>
          <class name="testCases.SignInPageTestCases"/>
           <class name="testCases.TryEditorTestCases"></class>
      </classes>
   </test>
</suite>