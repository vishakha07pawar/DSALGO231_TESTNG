<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DSAlgoTestSuite" parallel="classes" thread-count="5">
	<listeners>
		<listener
			class-name="com.aventstack.chaintest.plugins.ChainTestListener" />
		<listener class-name="listeners.ExtentReportTestNgListener" />
		<listener class-name="listeners.CustomListener" />
		<listener class-name="analyzer.MyTransformerAnalyzer" />
	</listeners>
	<test name="FirefoxTests">
		<parameter name="browserType" value="firefox" />
		<classes>
			<class name="testCases.DsAlgoPortalTestCases" />
			<class name="testCases.HomePageTestCases" />
			<class name="testCases.RegisterPageTestCases" />
			<class name="testCases.SignInPageTestCases" />
			<class name="testCases.HomePageAfterSignInTestCases" />
			<class name="testCases.ArrayPageTestCases" />
			<class name="testCases.StackPageTestCases" />
			<class name="testCases.QueuePageTestCases" />
			<class name="testCases.TreePageTestCases" />
			<class name="testCases.DataStructurePageTestCases" />
			<class name="testCases.LinkedListPageTestCases" />
			<class name="testCases.GraphPageTestCases" />
			<class name="testCases.TryEditorTestCases" />
		</classes>
	</test>
	<test name="EdgeTests">
		<parameter name="browserType" value="edge" />
		<classes>
			<class name="testCases.DsAlgoPortalTestCases" />
			<class name="testCases.HomePageTestCases" />
			<class name="testCases.RegisterPageTestCases" />
			<class name="testCases.SignInPageTestCases" />
			<class name="testCases.HomePageAfterSignInTestCases" />
			<class name="testCases.ArrayPageTestCases" />
			<class name="testCases.StackPageTestCases" />
			<class name="testCases.QueuePageTestCases" />
			<class name="testCases.TreePageTestCases" />
			<class name="testCases.DataStructurePageTestCases" />
			<class name="testCases.LinkedListPageTestCases" />
			<class name="testCases.GraphPageTestCases" />
			<class name="testCases.TryEditorTestCases" />
		</classes>
	</test>
	<test name="ChromeTests">
		<parameter name="browserType" value="chrome" />
		<classes>
			<class name="testCases.DsAlgoPortalTestCases" />
			<class name="testCases.HomePageTestCases" />
			<class name="testCases.RegisterPageTestCases" />
			<class name="testCases.SignInPageTestCases" />
			<class name="testCases.HomePageAfterSignInTestCases" />
			<class name="testCases.ArrayPageTestCases" />
			<class name="testCases.StackPageTestCases" />
			<class name="testCases.QueuePageTestCases" />
			<class name="testCases.TreePageTestCases" />
			<class name="testCases.DataStructurePageTestCases" />
			<class name="testCases.LinkedListPageTestCases" />
			<class name="testCases.GraphPageTestCases" />
			<class name="testCases.TryEditorTestCases" />
		</classes>
	</test>
</suite>